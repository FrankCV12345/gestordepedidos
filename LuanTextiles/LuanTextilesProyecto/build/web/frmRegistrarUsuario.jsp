<div class="ventana-registrar" id="ventana-registrar" >
                           <h1>REGISTRAR USUARIOO</h1>
                           <form >
                               <input type="text" placeholder="ingrese nombre " name="nombre" class="datos-cli" id="Nombre" required >
                               <input type="text" placeholder="ingrese apellido " name="apellidosUsuario" class="datos-cli" id="ApellidosUsuario" required>
                               <input type="number" placeholder="ingrese telefono " name="apellidosUsuario" class="datos-cli" id="telefonoUsuario" max="999999999" required>
                                <input type="text" placeholder="nombre de usuarioo" name="nomusu" class="datos-cli" id="nomusu" maxlength="8" required>
                                 <input type="text" placeholder="ingrese puesto de usuario" name="puesto" class="datos-cli" id="puestoR" required>
                              
                                 <input type="password" placeholder="ingres contraseña de usuario"  class="datos-cli" id="contraseniaA" maxlength="8" required>
                               <input type="password" placeholder="confirme contraseña" name="nombreUsuario" class="datos-cli" id="contrasenia" maxlength="8" required>
                            
                             <input type="reset" value="CERRAR" class="datos-pedido more btn" onclick="Despliega('ventana-registrar','padre-ventanas')">
                             <input type="submit" value="REGISTRAR" class="datos-pedido more btn" id="btnregistrar">
                             
                           </form>
                                </div>